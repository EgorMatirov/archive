<% c++ #include "bacs/archive/web/content/form.hpp" %>
<% skin plain %>
    <% view extract uses bacs::archive::web::content::extract extends base_form %>
        <% template subtitle() %>
            <% gt "Extract problems" %>
        <% end template %>
        <% template form() %>
            <p><% gt "Extract problems from archive" %></p>
            <form method="post" action="">
                <% form as_p form %>
            </form>
        <% end template %>
        <% template result() %>
            <!-- Always empty -->
        <% end template %>
    <% end view %>
<% end skin %>
